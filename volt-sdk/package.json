{
  "name": "@friktion-labs/friktion-sdk",
  "version": "1.1.119",
  "license": "proprietary",
  "repository": "https://github.com/Friktion-Labs/sdk",
  "scripts": {
    "start": "yarn idl && tsc -P tsconfig.build.json -w",
    "build": "tsc -P tsconfig.build.json",
    "clippy": "cargo clippy -- -A clippy::boxed_local -A unaligned_references",
    "idl": "yarn idl:generate",
    "delete-sourcemaps": "find ./dist -name '*.map' -delete",
    "idl:generate": "./scripts/fast-generate-idl.sh && eslint --fix src/idls",
    "idl:generate:nolint": "./scripts/fast-generate-idl.sh",
    "idl:watch": "nodemon -e rs --watch ./programs --exec 'yarn idl'",
    "dump": "./scripts/take-a-dump.sh",
    "lint": "eslint src/ --cache",
    "lint:fix": "eslint src/ --cache --fix",
    "lint:ci": "eslint src/ --max-warnings=0",
    "release": "git add package.json && node ./scripts/generate-release-commit-message.js && git commit --allow-empty -F ./scripts/release-message && rm ./scripts/release-message && echo '\n\nüôè Please run git push with this latest commit at HEAD, or include the commit message in the pull request title'",
    "bump": "yarn version patch && yarn release",
    "format:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "test:all": "anchor build && yarn test:legacy && yarn test:modern",
    "test:volt": "ANCHOR_WALLET=~/.config/solana/id.json anchor test --skip-build",
    "test:volt-ci": "ANCHOR_WALLET=~/.config/solana/id.json bin/test-volt-catch-error.sh",
    "test:inertia": "ANCHOR_WALLET=~/.config/solana/id.json cd tests/integration/inertia-suite && anchor test --skip-build",
    "test:inertia-ci": "ANCHOR_WALLET=~/.config/solana/id.json bin/test-inertia-catch-error.sh",
    "test:swap": "ANCHOR_WALLET=~/.config/solana/id.json cd tests/integration/swap-suite && anchor test --skip-build",
    "test:unit": "ANCHOR_WALLET=~/.config/solana/volt-testing.json yarn run ts-mocha -p ./tests/tsconfig.json -t 100000 tests/unit/**/*test*.ts",
    "test:serum": "ANCHOR_WALLET=~/.config/solana/id.json anchor test --skip-build tests/serum/proxy.ts",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "dependencies": {
    "@blockworks-foundation/mango-client": "3.4.5",
    "@friktion-labs/entropy-client": "^1.7.3",
    "@oclif/command": "^1.8.16",
    "@project-serum/anchor": "^0.25.0-beta.1",
    "@project-serum/common": "^0.0.1-beta.3",
    "@project-serum/serum": "^0.13.61",
    "@saberhq/anchor-contrib": "1.12.53",
    "@saberhq/solana-contrib": "1.12.53",
    "@saberhq/token-utils": "1.12.53",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.42.0",
    "@switchboard-xyz/switchboard-v2": "^0.0.34",
    "bn.js": "^5.2.1",
    "csv-parse": "^5.0.4",
    "decimal.js": "^10.3.1",
    "fast-csv": "^4.3.6",
    "superagent": "^7.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.1",
    "@commitlint/config-angular": "17.0.0",
    "@rushstack/eslint-patch": "^1.1.0",
    "@saberhq/eslint-config": "1.13.0",
    "@types/bn.js": "^5.1.0",
    "@types/mocha": "^9.0.0",
    "@types/superagent": "4.1.15",
    "chai": "4.3.6",
    "eslint": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.25.3",
    "husky": "^8.0.1",
    "jsbi": "4.3.0",
    "mocha": "10.0.0",
    "nodemon": "^2.0.15",
    "pinst": "^3.0.0",
    "prettier": "^2.5.1",
    "ts-mocha": "10.0.0",
    "ts-node": "10.8.0"
  },
  "main": "dist/cjs/src/index.js",
  "files": [
    "dist",
    "dist/cjs"
  ],
  "packageManager": "yarn@3.1.0",
  "resolutions": {
    "@saberhq/chai-solana/@saberhq/solana-contrib": "1.13.0",
    "@saberhq/token-utils/@saberhq/solana-contrib": "1.13.0",
    "@saberhq/eslint-config/@saberhq/solana-contrib": "1.13.0"
  }
}
